{
  "name": "notivra-blog",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "build:book": "astro build --config astro.print.config.mjs",
    "pdf": "weasyprint press/build/print/bookone/index.html \"$npm_package_config_pdf\"",
    "docx": "pandoc press/build/print/bookone/index.html --reference-doc=press/reference.docx -o \"$npm_package_config_doc\"",
    "md": "pandoc press/build/print/bookone/index.html -o press/editable/Mastering_R.md"
  },
  "config": {
    "pdf": "press/pdf/Mastering R From Data to Clarity.pdf",
    "doc": "press/pdf/Mastering R From Data to Clarity.docx"
  },
  "dependencies": {
    "@astrojs/mdx": "^4.3.9",
    "@astrojs/rss": "^4.0.13",
    "@astrojs/sitemap": "^3.6.0",
    "@astrojs/solid-js": "^5.1.2",
    "@tailwindcss/typography": "^0.5.18",
    "@tailwindcss/vite": "^4.1.13",
    "astro": "^5.15.3",
    "sharp": "^0.34.3",
    "solid-js": "^1.9.10",
    "tailwindcss": "^4.1.13"
  }
}
